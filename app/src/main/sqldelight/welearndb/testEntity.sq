CREATE TABLE testEntity (
    id INTEGER PRIMARY KEY NOT NULL,
    Points1 TEXT,
    Points2 TEXT,
    Points3 TEXT,
    Points4 TEXT,
    Points5 TEXT,
    Question TEXT,
    Answer1 TEXT,
    Answer2 TEXT,
    Answer3 TEXT,
    Answer4 TEXT,
    Answer5 TEXT);

getAllQuestions:
SELECT *
FROM testEntity;

insertQuestion:
INSERT OR REPLACE
INTO testEntity
VALUES(?,?,?,?,?,?,?,?,?,?,?,?);

deleteQuestions:
DELETE FROM testEntity;
